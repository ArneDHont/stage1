  <DataSets>
    <DataSet Name="dsMaandDGPO">
      <Query>
        <DataSourceName>bbw</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Maand">
            <Value>=Parameters!Maand.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Jaar">
            <Value>=Parameters!Jaar.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     dbo.BBDGPODL.VLG, dbo.BBDGPODL.SCF_DG_PO_DL, SUM(dbo.BBDGPO.Q_PO_1) AS sum_Q_PO_1, SUM(dbo.BBDGPO.Q_PO_2) AS sum_Q_PO_2, 
                      SUM(dbo.BBDGPO.Q_PO_4) AS sum_Q_PO_4, SUM(dbo.BBDGPO.Q_PO_ALG) AS sum_Q_PO_ALG
FROM         dbo.BBDGPO INNER JOIN
                      dbo.BBDGPODL ON dbo.BBDGPO.ID_DG_PO_DL = dbo.BBDGPODL.ID_DG_PO_DL
WHERE     (MONTH(dbo.BBDGPO.TMS_REG_DG_PO) = @Maand) AND (YEAR(dbo.BBDGPO.TMS_REG_DG_PO) = @Jaar)
GROUP BY dbo.BBDGPO.ID_DG_PO_DL, dbo.BBDGPODL.SCF_DG_PO_DL, dbo.BBDGPODL.VLG
ORDER BY dbo.BBDGPODL.VLG</CommandText>
      </Query>
