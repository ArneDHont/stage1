        <Field Name="VLG_NR">
          <Value>=Fields!JAARTAL.Value &amp; "/BW " &amp; Fields!VLG_TY_INTV_JR.Value &amp; "/" &amp; Fields!KRT_AFD.Value &amp; " " &amp; Fields!VLG_TY_INTV_AFD.Value</Value>
        </Field>
