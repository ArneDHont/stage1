        <CommandText>SELECT     dbo.BBAFD.KRT_AFD, dbo.BBINTVBR.PLA_INTV, CONVERT(char(8), dbo.BBINTVBR.TMS_INTV, 3) AS TMS_INTV, dbo.BBINTVTY.SCF_TY_INTV, 
                      CONVERT(char(8), dbo.BBINTVBR.DT_OPS_RAP_INTV, 3) AS DT_OPS_RAP_INTV, YEAR(dbo.BBINTVBR.TMS_INTV) AS JAARTAL, 
                      dbo.BBINTVBR.VLG_TY_INTV_JR, dbo.BBINTVBR.VLG_TY_INTV_AFD
FROM         dbo.BBINTVBR INNER JOIN
                      dbo.BBINTVTY ON dbo.BBINTVTY.ID_TY_INTV = dbo.BBINTVBR.ID_TY_INTV INNER JOIN
                      dbo.BBIND ON dbo.BBIND.ID_IND = dbo.BBINTVBR.ID_OPS INNER JOIN
                      dbo.BBBRRD ON dbo.BBINTVBR.ID_BR_RD_INTV = dbo.BBBRRD.ID_BR_RD_INTV INNER JOIN
                      dbo.BBAFD ON dbo.BBAFD.ID_AFD = dbo.BBINTVBR.ID_AFD INNER JOIN
                      dbo.BBBRTY ON dbo.BBINTVBR.ID_BR_TY_INTV = dbo.BBBRTY.ID_BR_TY_INTV
WHERE     (dbo.BBINTVBR.ID_INTV_BRDW = @ID_INTV_BRDW)</CommandText>
      </Query>
      <Fields>
