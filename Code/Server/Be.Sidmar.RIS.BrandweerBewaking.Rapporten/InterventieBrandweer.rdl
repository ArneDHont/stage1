        <DataSourceName>bbw</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ID_INTV_BRDW">
            <Value>=Parameters!ID_INTV_BRDW.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     aanspr.SCF_IND_GSL_RAP + ' ' + RTRIM(ind.VNM_IND) + ' ' + RTRIM(UPPER(ind.NM_IND)) AS NM_OPS, '(' + ind.SAP_AFD + ')' AS SAP_AFD
FROM         dbo.BBIND AS ind INNER JOIN
                      dbo.BBINDGSL AS aanspr ON aanspr.ID_GSL_IND = ind.ID_GSL_IND INNER JOIN
                      dbo.BBINTVBR AS intv ON intv.ID_OPS = ind.ID_IND
WHERE     (intv.ID_INTV_BRDW = @ID_INTV_BRDW)</CommandText>
      </Query>
