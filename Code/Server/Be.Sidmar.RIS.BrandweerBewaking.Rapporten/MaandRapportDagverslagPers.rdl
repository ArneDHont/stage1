  <DataSets>
    <DataSet Name="bbw">
      <Query>
        <DataSourceName>bbw</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Maand">
            <Value>=Parameters!Maand.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Jaar">
            <Value>=Parameters!Jaar.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     BBDGTYREG.SCF_DG_PERS_TY_REG, COUNT(BBDGPERS.ID_DG_PERS) AS Aantal
FROM         BBDGPERS INNER JOIN
                      BBDGTYREG ON BBDGPERS.ID_DG_PERS_TY_REG = BBDGTYREG.ID_DG_PERS_TY_REG
WHERE     MONTH(tms_reg_dg_pers) = @Maand AND YEAR(tms_reg_dg_pers) = @Jaar
GROUP BY BBDGTYREG.SCF_DG_PERS_TY_REG</CommandText>
      </Query>
