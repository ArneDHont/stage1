  <Code>
Public Shared Function GetMonthName(ByVal maand As Integer) As String
		Dim monthNr As Integer = maand
		Select Case monthNr
			Case 1
				Return "januari"
			Case 2
				Return "februari"
			Case 3
				Return "maart"
			Case 4
				Return "april"
			Case 5
				Return "mei"
			Case 6
				Return "juni"
			Case 7
				Return "juli"
			Case 8
				Return "augustus"
			Case 9
				Return "september"
			Case 10
				Return "oktober"
			Case 11
				Return "november"
			Case 12
				Return "december"
		End Select
End Function</Code>
