    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="bbw">
      <Query>
        <DataSourceName>bbw</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@maand">
            <Value>=Parameters!maand.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@jaar">
            <Value>=Parameters!jaar.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT g.ActieCodeGroepOmschr, a.ActieOmschr, p.scf_plg_ind, sum(d.AantalUur) AS "Uren", sum(d.AantalMinuten) AS "Minuten"

FROM dbo.bvActieGroep AS g
INNER JOIN dbo.bvActie AS a ON a.ActieCodeGroepId = g.ActieCodeGroepId
INNER JOIN dbo.bvDagVerslagActie AS d ON d.ActieCodeId = a.ActieCodeId
INNER JOIN dbo.bvDagVerslag AS v ON v.DagVerslagId = d.DagVerslagId
INNER JOIN dbo.BBBWPLG AS p ON p.id_plg_ind = v.PloegId

WHERE g.ActieCodeGroepId &lt; 11
  AND a.InDagVerslagYN = 1
  AND month(v.Datum) = @maand
  AND year(v.Datum) = @jaar

GROUP BY g.ActieCodeGroepOmschr, a.ActieOmschr, p.scf_plg_ind

UNION ALL

SELECT g.ActieCodeGroepOmschr, a.ActieOmschr, '060', 0, 0
FROM dbo.bvActieGroep AS g
INNER JOIN dbo.bvActie AS a ON a.ActieCodeGroepId = g.ActieCodeGroepId

WHERE g.ActieCodeGroepId &lt; 11
  AND a.InDagVerslagYN = 1
  AND NOT EXISTS( SELECT *
		FROM dbo.bvDagVerslagActie AS d 
		INNER JOIN dbo.bvDagVerslag AS v ON v.DagVerslagId = d.DagVerslagId
		WHERE d.ActieCodeId = a.ActieCodeId
		AND month(v.Datum) = @maand
		AND year(v.Datum) = @jaar )
 
ORDER BY 1, 2, 3</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
